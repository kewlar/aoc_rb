# frozen_string_literal: true

RSpec.describe Year<%= @year %>::Day<%= @day %> do
  let(:input) { File.read(File.join(__dir__, '../../../challenges/<%= @year %>/<%= @day.to_s.rjust(2, '0') %>/input.txt')) }
  let(:example_input) {
    <<~EOF
      change
      me
    EOF
  }

  describe 'part 1' do
    it 'solves for the example input' do
      expect(described_class.part_1(example_input)).to eq(nil)
    end

    xit 'solves for the full input' do
      expect(described_class.part_1(input)).to eq(nil)
    end
  end

  xdescribe 'part 2' do
    it 'solves for the example input' do
      expect(described_class.part_2(example_input)).to eq(nil)
    end

    xit 'solves for the full input' do
      expect(described_class.part_2(input)).to eq(nil)
    end
  end
end
